{
  "name": "WunderFocus",
  "description": "A Chrome extension to focus on the current task in Wunderlist.",
  "version": "0.1",
  "background": {
    "scripts": ["jquery.js","background.js","wunderfocus.js"]
  },
  "permissions": [
    "tabs", "https://www.wunderlist.com/*"
  ],
  "browser_action": {
      "default_title": "WunderFocus",
      "default_icon": "wunderfocus48.png"
  },
  "icons": {
    "16": "wunderfocus16.png",
	"48": "wunderfocus48.png"
  },
  "manifest_version": 2
}
